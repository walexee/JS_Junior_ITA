<div class="panel panel-primary">
    <div class="panel-heading">
        <div class="row">
            <div class="col-xs-6">
                <h3 class="panel-title">Employees</h3>
            </div>
            <div class="pull-right">
                <button role="button" class="btn btn-default">Add</button>
                <button role="button" class="btn btn-default">Edit</button>
                <button role="button" class="btn btn-default">Delete</button>
            </div>
        </div>
    </div>
    <div class="panel-body">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Job Title</th>
                    <th>Email</th>
                    <th>Phone #</th>
                </tr>
            </thead>
            <tbody id="employee-rows">
                <!--employee rows here-->
            </tbody>
        </table>
    </div>
</div>

@section scripts {
    <script>
        $(app.employees.init)
    </script>
}